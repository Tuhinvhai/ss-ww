 *{
	margin:0;
	padding:0;
	box-sizing:border-box;
	
}
.container{
	max-width:1300px;
	width:100%;
	margin:0 auto;
	
}


body{
	font-family: 'Open Sans', sans-serif;
	
}   
ul li{
	list-style:none;
	
}

/*=================== main prat topber ====================*/
.topber-section .top-parrent{
	 
	 display:flex;
	 justify-content:space-between;
	
}
.topber-section .nuber ul{
	
	display:flex;
	
}
.topber-section .number2 ul{
	
	display:flex;
	
}
.topber-section{
	background:#173a69;
	padding:10px 0px;
	
}

.topber-section .nuber ul .dibba{
	 margin-right:30px;
	
	
}   
.topber-section .nuber li span{
	margin-left:8px;
	
}
.topber-section .nuber li a{
	color:white;
	text-decoration:none;
	
}

.topber-section .number2 ul li{
	margin-left:30px;
	
}
.topber-section .number2 ul li a{
	color:white;
	
}
.topber-section .top-parrent ul li a:hover{
	
	color:yellow;
	transition:0.1s;

	
}

/*======================== header part =====================*/



.header-section{
	padding:20px 0;
	
	
	
}

.header-section .header-logo img{
	width:200px;
	height:auto;

}

.header-section .header-babu{
	display:flex;
	justify-content:space-between;
	align-items:center;
	
	
}
.header-section .header-nav ul{
	display:flex;
   
	
}
.header-section .header-nav ul li{
	margin-left:79px;

	
	
}
.header-section .header-nav ul li a{
	
	text-transform:uppercase;
	text-decoration:none;
	color:#0c1f38;
	position:relative;
	font-weight:bold;
	
}

.header-section .header-nav ul li a:after{
	
	content:"";
	position:absolute;
	display:block;
	right:0;
	bottom:-61px;
	background-color:black;
	width:0%;
	height:6px;
	
	
	
}

.header-section .header-nav ul li:hover a::after{
	
	width:100%;
	transition:.3s;
	
}

.header-section .header-nav .active{
position:relative;  
	
}
.header-section .header-nav .active::after{
	

	content:"";
	position:absolute;
	display:block;
	right:0;
	bottom:-61px;
	background-color:black;
	width:100%;
	
	
	  
	
}

/*=================== banner section =================*/


.banner-section{
	
	 background-image: url("../img/ggg.jpg");
     background-size:100%;
     background-repeat:no-repeat;	
     width:auto;
     height:531px;
	 padding:163px 0;
	 position:relative;
	 z-index:1;
}
.banner-section::after{
	 content:"";
     position:absolute;
     background-image: url("../img/back.jpg");
     background-size:cover;
     background-repeat:no-repeat;	
	 background-position:center;
     width:100%;
     height:100%;
	z-index:-1;
	 top:0;
	 left:0;
	 opacity:.2;
}




 .banner-section .banner {
   position: absolute;
  top: 50%;
  left: 70%;
  transform: translate(-50%, -50%);
	

}
.banner-section .banner h2{
	 font-weight: bold;
	 font-size:17px;	 
	 color:#222222;
     text-transform:uppercase;
	 margin-bottom:14px;
}
.banner-section .banner h1{
	font-size:60px;
	text-transform:uppercase;
	font-weight:800;
	margin-bottom:20px;
	
	
	
}
.banner-section .banner p{
	 
margin:10px 0;
	 
}   
.banner-section .banner .btn button{

margin-top:30px;
	
}
.banner-section .banner .btn button a{
	text-decoration:none;
	text-transform:uppercase;
	
	
	
}

#btn1{
border:none;}

#btn1 a {
    color: white;
    padding: 15px 30px;
    display: inline-block;
    border: 2px solid transparent;
	background:#173a69;
	font-weight:bold;
	transition:0.3s;
}

#btn1 a:hover{
	background:#fff;
	border: 2px solid #173a69;
	color:#173a69;
}
	
#btn2{
	margin-left:20px;
	border:none;
	background:#fefeff;


}
#btn2 a {
    display: block;
    color: #173a69;
    font-weight: 700;
    display: inline-block;
    padding: 15px 30px;
    border: 2px solid #173a69;
    background: #fff;
	transition:0.3s;
}
#btn2 a:hover{
	color:#fff;
	background-color:#173a69;
   
}








