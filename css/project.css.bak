/*====================gallary section======================*/
.gallary-section{
	padding-bottom:;
	
	
}


.gallary-section .badluck{
	text-align:center;
   
	font-size:28px;
	font-weight:bold;
	text-transform:uppercase;
	
}
.gallary-section .badluck img{
	
     
	
	
}

.gallary-section .badluck h3{
	
	margin:30px 0;
	
	
}



.gallary-section .gallary-baba{
	
	display:grid;
	grid-template-columns:1fr 1fr 1fr;
    column-gap:30px;
	row-gap:30px;
	margin-top:85px;
}
.gallary-section .gallary-baba img{
	
	width:100%;
	
	
}


.gallary-section .gallary-items{
	text-align:center;
    position:relative;	
  


}


.gallary-section .hover-contant{
	
	position:absolute;
	top:50%;
	left:50%;
	height:50%;
	
	transform: translate(-50px, -50px);
	 opacity:0;
	 transition:.6s;
     color:white;
     z-index:1;
}

.gallary-section .gallary-items:hover .hover-contant{
	
	opacity:1;
  
	
   
	
	
}



.gallary-section .gallary-items h5{
	
	font-size:26px;
	font-weight:700;
	
	

}

.gallary-section .gallary-items::after{
	
	content:"";
    position:absolute;
	width:100%;
	height:98%;
	background:black;
	top:0;
	left:0;
	transition: 2s;
	opacity:0;
	color:white;


  
	
}


.gallary-section .gallary-items:hover::after{
	
	width:100%;
	opacity:.6;
	 

    
	
    
}





